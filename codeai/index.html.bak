<script>
(function(){
  const API_BASE = String(window.CODEAI_API_BASE || "https://api.codeai.co.kr").replace(/\/+$/g, "");
  const ENDPOINTS = {
    createStudent: API_BASE + "/api/v1/public/student-applications",
    listInstructors: API_BASE + "/api/v1/public/instructors",
    selectInstructor: (id)=> API_BASE + "/api/v1/public/student-applications/" + encodeURIComponent(id) + "/select-instructor",
    phone: "tel:01048623239"
  };

  const fab = document.getElementById("codeaiChatFab");
  const widget = document.getElementById("codeaiChatWidget");
  const body = document.getElementById("codeaiChatBody");
  const quick = document.getElementById("codeaiChatQuick");
  const input = document.getElementById("codeaiChatInput");
  const sendBtn = document.getElementById("codeaiChatSend");
  const closeBtn = document.getElementById("codeaiChatClose");
  const restartBtn = document.getElementById("codeaiChatRestart");
  if(!fab || !widget || !body || !quick || !input || !sendBtn || !closeBtn || !restartBtn) return;

  let state = { step:"name", studentAppId:null, answers:{} };

  function el(tag, cls, text){
    const n = document.createElement(tag);
    if(cls) n.className = cls;
    if(text !== undefined && text !== null) n.textContent = text;
    return n;
  }
  function scrollToBottom(){ body.scrollTop = body.scrollHeight; }

  function addMsg(who, text){
    const row = el("div", "codeai-msg " + (who==="me" ? "me":"bot"));
    const bubble = el("div", "codeai-bubble", text);
    row.appendChild(bubble);
    body.appendChild(row);
    scrollToBottom();
  }
  function addBotNode(node){
    const row = el("div","codeai-msg bot");
    const bubble = el("div","codeai-bubble");
    bubble.appendChild(node);
    row.appendChild(bubble);
    body.appendChild(row);
    scrollToBottom();
  }

  function setQuick(items){
    quick.innerHTML = "";
    (items||[]).forEach(it=>{
      const b = el("button","codeai-quick-btn", it.label);
      b.type = "button";
