  async function handleInput(text){
    const t = String(text||"").trim();
    if(!t) return;

    if(t === "ì²˜ìŒ"){
      reset();
      return;
    }

    if(state.step === "name"){
      state.answers.name = t;
      addMsg("me", t);
      state.step = "phone";
      ask();
      return;
    }

    if(state.step === "phone"){
      const p = normPhone(t);
      if(p.length < 10){
        addMsg("bot","íœ´ëŒ€í° ë²ˆí˜¸ê°€ ì˜¬ë°”ë¥´ì§€ ì•Šì•„ìš”. ë‹¤ì‹œ ì…ë ¥í•´ ì£¼ì„¸ìš”.");
        return;
      }
      state.answers.phone = p;
      addMsg("me", t);
      state.step = "subjects";
      ask();
      return;
    }

    if(state.step === "subjects"){
      const parts = t.split(/[,/
]/).map(s=>s.trim()).filter(Boolean);
      if(!parts.length){
        addMsg("bot","ê³¼ëª©ì„ 1ê°œ ì´ìƒ ì…ë ¥í•´ ì£¼ì„¸ìš”.");
        return;
      }
      state.answers.subjects = parts.slice(0, 5);
      addMsg("me", parts.join(", "));
      state.step = "target";
      ask();
      return;
    }

    if(state.step === "target"){
      state.answers.target = t;
      addMsg("me", t);
      state.step = "mode";
      ask();
      return;
    }

    if(state.step === "mode"){
      if(!["ZOOM","OFFLINE_1_1","OFFLINE_GROUP"].includes(t)){
        addMsg("bot","ì•„ë˜ ë²„íŠ¼ìœ¼ë¡œ ì„ íƒí•´ ì£¼ì„¸ìš”.");
        return;
      }
      state.answers.mode = t;
      addMsg("me", t === "ZOOM" ? "ì¤Œ(ì „êµ­)" : (t === "OFFLINE_1_1" ? "ì˜¤í”„ë¼ì¸ 1:1" : "ì˜¤í”„ë¼ì¸ ê·¸ë£¹"));
      if(t === "ZOOM"){
        state.answers.region = null;
        state.step = "confirm";
      }else{
        state.step = "region";
      }
      ask();
      return;
    }

    if(state.step === "region"){
      state.answers.region = t;
      addMsg("me", t);
      state.step = "confirm";
      ask();
      return;
    }

    if(state.step === "confirm"){
      if(t === "ì‹ ì²­í•˜ê¸°"){
        addMsg("me","ì‹ ì²­í•˜ê¸°");
        addMsg("bot","ì‹ ì²­ì„ ì ‘ìˆ˜ ì¤‘ì…ë‹ˆë‹¤â€¦");
        setQuick([]);
        try{
          await submitStudentApplication();
        }catch(e){
          addMsg("bot","ì‹ ì²­ ì‹¤íŒ¨: " + e.message + "
ì ì‹œ í›„ ë‹¤ì‹œ ì‹œë„í•´ ì£¼ì„¸ìš”.");
          state.step = "confirm";
          ask();
        }
        return;
      }
      addMsg("bot","'ì‹ ì²­í•˜ê¸°' ë˜ëŠ” 'ì²˜ìŒ'ì„ ì…ë ¥í•´ ì£¼ì„¸ìš”.");
      return;
    }
  }

  function reset(){
    body.innerHTML = "";
