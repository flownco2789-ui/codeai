  function open(){ widget.classList.add("open"); fab.classList.add("hide"); input.focus(); }
  function close(){ widget.classList.remove("open"); fab.classList.remove("hide"); }

  
  function openAndInit(){
    open();
    if(body.childElementCount===0) reset();
  }

  // Header / CTA links: open chat instead of only moving anchor
  document.querySelectorAll(".js-open-enroll-chat").forEach((el)=>{
    el.addEventListener("click", (e)=>{
      // On index.html we can open immediately
      e.preventDefault();
      // keep hash for share/back button
      if(location.hash !== "#enroll"){ history.replaceState(null, "", "#enroll"); }
      openAndInit();
    });
  });

  // If user navigates to index.html#enroll from another page, open automatically
  function autoOpenFromHash(){
    if(location.hash === "#enroll"){
      openAndInit();
    }
  }
  window.addEventListener("hashchange", autoOpenFromHash);
  // run once on load
  setTimeout(autoOpenFromHash, 0);


  fab.addEventListener("click", ()=>{ open(); if(body.childElementCount===0) reset(); });
