  async function renderInstructorList(){
    const a = state.answers;
    const qs = new URLSearchParams();
    if(a.subjects && a.subjects[0]) qs.set("subject", a.subjects[0]); // MVP: ì²« ê³¼ëª© ê¸°ë°˜
    if(a.mode) qs.set("mode", a.mode);
    if(a.region) qs.set("region", a.region);

    let out;
    try{
      out = await getJson(ENDPOINTS.listInstructors + "?" + qs.toString());
    }catch(e){
      addMsg("bot","ê°•ì‚¬ ëª©ë¡ì„ ë¶ˆëŸ¬ì˜¤ì§€ ëª»í–ˆìŠµë‹ˆë‹¤: " + e.message);
